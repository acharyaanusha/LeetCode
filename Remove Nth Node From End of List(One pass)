# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def removeNthFromEnd(self, head: ListNode, n: int) -> ListNode:
        dummy = ListNode(0)
        dummy.next=head
        first=dummy
        second=dummy
        j=0
        while j<=n:
            second = second.next
            j+=1
        while second:
            first=first.next
            second=second.next   
        first.next=first.next.next
        return dummy.next
