class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        count = 0
        result=0
        for i in range(len(nums)):
            if  nums[i]==1:
                count = count+1
                if(count>result):
                    result=count
            else:
                if(result>=len(nums)/2):
                    return (result)
                count = 0
        return result
